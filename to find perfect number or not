#include <stdio.h>
int main()
{
    int n;
    int i;
    int sum=0;
    printf("enter any number");
    scanf("%d",&n);
    for(i=1;i<=n/2;i++)
    {
        if(n%i==0)
        {
            sum=sum+i;
        }
    }
    if(sum==n)
    {
        printf("\n the given number is perfect");
    }
    else
    {
        printf("\n yhe given number not a perfect");
    }
}
