#include<stdio.h>
int main()
{
    int size;
    scanf("%d",&size);
    int arr[size],index;
    for(index=0;index<size;index++)
    {
        scanf("%d",&arr[index]);
    }
    int max_ele=arr[0];
    for(index=1;index<size;index++)
    {
        if(max_ele<arr[index])
        {
            max_ele=arr[index];
        }
    }
    printf("%d",max_ele);
}
